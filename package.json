{
    "name": "tos-ts",
    "displayName": "thinkScript",
    "description": "Syntax highlighting for the thinkScript language for thinkorswim trading platform",
    "version": "0.0.3",
    "publisher": "LethalArts",
    "homepage": "https://github.com/jmbeekman/vsc-thinkscript-extension",
    "icon": "images/tosicon.jpg",
    "repository": {
        "type": "git",
        "url": "https://github.com/jmbeekman/vsc-thinkscript-extension.git"
    },
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "thinkscript",
                "aliases": [
                    "thinkScript",
                    "thinkscript"
                ],
                "extensions": [
                    ".tosts"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "thinkscript",
                "scopeName": "source.tosts",
                "path": "./syntaxes/thinkscript.tmLanguage.json"
            }
        ]
    }
}
